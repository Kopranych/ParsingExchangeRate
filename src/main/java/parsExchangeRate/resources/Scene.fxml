<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="220.0" prefWidth="220.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="parsExchangeRate.resources.InterfaceController">
   <children>
      <AnchorPane prefHeight="220.0" prefWidth="220.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <GridPane prefHeight="150.0" prefWidth="200.0" AnchorPane.leftAnchor="4.0" AnchorPane.rightAnchor="4.0" AnchorPane.topAnchor="4.0">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="104.0" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="137.0" minWidth="10.0" prefWidth="100.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="37.0" minHeight="10.0" prefHeight="35.0" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="37.0" minHeight="10.0" prefHeight="29.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <Button mnemonicParsing="false" onAction="#getChart" text="Chart" GridPane.rowIndex="3" />
                  <Button mnemonicParsing="false" onAction="#getActualExchangeRate" text="Get" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <Label text="USD" />
                  <Label text="EUR" GridPane.columnIndex="1" />
                  <Label text="Date" GridPane.rowIndex="2" />
                  <TextArea fx:id="usdArea" prefHeight="200.0" prefWidth="200.0" promptText="USD" GridPane.rowIndex="1" />
                  <TextArea fx:id="eurArea" prefHeight="200.0" prefWidth="200.0" promptText="EUR" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextArea fx:id="dateArea" prefHeight="200.0" prefWidth="200.0" promptText="date" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               </children>
            </GridPane>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
